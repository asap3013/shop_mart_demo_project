  context = {}
    if request.method == "POST":
        form = BannersForm(request.FILES,request.POST)
        print(form)
        if form.is_valid():
            print("hello")
            img = form.cleaned_data.get("banner_path")
            status = form.cleaned_data.get("status")
            obj = Banners.objects.create(
                                 banner_path = img,
                                 status = status
                                 )
            obj.save()
            print(obj)
    else:
        print("world")
        form = BannersForm()
    context['form']= form





